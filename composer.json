{
    "name": "hexydec/html-minify-test",
    "description": "A script to compare the performance of HTML minifiers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Earp",
            "email": "will@hexydec.com",
			"homepage": "https://github.com/hexydec"
        }
    ],
	"repositories": [
	    {
	        "type":"package",
	        "package": {
	            "name": "taufik-nurrohman/php-html-css-js-minify",
	            "version": "master",
	            "source": {
	                "url": "https://gist.github.com/taufik-nurrohman/d7b310dea3b33e4732c0",
	                "type": "git",
	                "reference":"master"
	            },
	            "autoload": {
					"files": ["php-html-css-js-minifier.php"]
	            }
	        }
	    }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
		"hexydec/htmldoc": "@dev",
        "arjanschouten/htmlminifier": "^0.0.2",
        "voku/html-min": "^4.4",
        "wyrihaximus/html-compress": "^4.1",
        "mrclay/minify": "^3.0",
        "matthiasmullie/minify": "^1.3",
        "tedivm/jshrink": "^1.4",
        "tubalmartin/cssmin": "^4.1",
        "pfaciana/tiny-html-minifier": "^3.0",
        "deruli/html-minifier": "^0.5.1",
        "fisharebest/algorithm": "^1.5",
        "natxet/cssmin": "^3.0",
        "cerdic/css-tidy": "^1.7",
        "websharks/css-minifier": "150820",
        "wikimedia/minify": "^2.2",
		"taufik-nurrohman/php-html-css-js-minify": "dev-master"
	},
	"autoload": {
		"classmap": ["src/"]
	},
    "require-dev": {
        "phpunit/phpunit": "9.5"
    }
}
